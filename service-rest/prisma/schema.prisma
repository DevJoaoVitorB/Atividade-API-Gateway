generator client {
  provider = "prisma-client"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model File {
  id        Int       @id @default(autoincrement())
  name      String
  size      Int
  owner     String
  createdAt DateTime  @default(now())
  updatedAt DateTime  @updatedAt   
}