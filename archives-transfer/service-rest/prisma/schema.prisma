generator client {
  provider = "prisma-client"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model File {
  id        Int      @id @default(autoincrement())
  name      String
  size      Int
  owner     String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}